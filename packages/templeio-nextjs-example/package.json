{
  "name": "@templeio/templeio-nextjs-example",
  "version": "0.3.1-next.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "plop": "plop",
    "commit": "cz",
    "release": "standard-version",
    "pre-release": "standard-version --prerelease",
    "prepare:comment": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{css,pcss,scss}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.7.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.1",
    "next": "^12.0.4",
    "next-i18next": "^9.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3"
  },
  "devDependencies": {
    "@templeio/eslint-config-templeio": "latest",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "commitizen": "^4.2.4",
    "inquirer-directory": "^2.2.0",
    "lint-staged": "^12.1.2",
    "npm-check": "^5.9.2",
    "plop": "^3.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "typescript": "^4.5.2"
  }
}
